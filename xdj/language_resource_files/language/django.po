msgstr "Không có loại vấn đề phù hợp trong các thư viện quy định."

#: common/lib/xmodule/xmodule/library_content_module.py
msgid "Select another problem type."
msgstr "Chọn một dạng bài tập khác."

#: common/lib/xmodule/xmodule/library_content_module.py
#, python-brace-format
